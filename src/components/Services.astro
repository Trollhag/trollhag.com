  <script>
    import gsap, { ScrollTrigger } from "gsap/all";
    gsap.registerPlugin(ScrollTrigger);
    gsap.from("#services [data-animate='1']", {
      y: 60,
      opacity: 0,
      stagger: 0.25,
      scrollTrigger: {
        trigger: "#services",
        start: "20% bottom",
        end: "bottom bottom",
        scrub: true,
      },
    });
    gsap.from("#services [data-animate='2']", {
      y: 60,
      opacity: 0,
      stagger: 0.25,
      scrollTrigger: {
        trigger: "#services",
        start: "30% bottom",
        end: "bottom bottom",
        scrub: true,
      },
    });
  </script>

